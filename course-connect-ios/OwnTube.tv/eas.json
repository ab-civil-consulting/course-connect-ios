{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "prebuildCommand": "npx expo prebuild --clean",
      "env": {
        "npm_config_legacy_peer_deps": "true",
        "EXPO_PUBLIC_NOTIFICATION_SERVER_URL": "https://course-connect.ab-civil.com/notifications"
      }
    },
    "preview": {
      "distribution": "internal",
      "prebuildCommand": "npx expo prebuild --clean",
      "env": {
        "npm_config_legacy_peer_deps": "true",
        "EXPO_PUBLIC_NOTIFICATION_SERVER_URL": "https://course-connect.ab-civil.com/notifications"
      }
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false,
      "prebuildCommand": "npx expo prebuild --clean",
      "env": {
        "npm_config_legacy_peer_deps": "true",
        "EXPO_PUBLIC_PRIMARY_BACKEND": "course-connect.ab-civil.com",
        "EXPO_PUBLIC_APP_NAME": "MC Assist",
        "EXPO_PUBLIC_APP_SLUG": "course-connect",
        "EXPO_PUBLIC_NOTIFICATION_SERVER_URL": "https://course-connect.ab-civil.com/notifications"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
